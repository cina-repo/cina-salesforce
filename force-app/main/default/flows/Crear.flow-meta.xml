    <formulas>
        <name>codigoBeneficioExtraccionDDDKEY</name>
        <dataType>String</dataType>
        <expression>&quot;CUR&quot; &amp; RIGHT({!$Record.DDDKEY__c}, LEN({!$Record.DDDKEY__c}) - 8)</expression>
    </formulas>
    <formulas>
        <name>crearPDDDKEYMatriculasFechas</name>
        <dataType>String</dataType>
        <expression>&quot;MAT0&quot;&amp;{!$Record.MM_ALUMNO__c}&amp; TEXT({!$Record.MM_ANO__c})&amp;{!$Record.MM_CURSO__c}&amp; {!$Record.MM_NIVEL__c}</expression>
    </formulas>
    <formulas>
        <name>Estado_Matriculas</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.MM_ANULA__c}=&apos;true&apos;,&apos;Cancelada&apos;, &apos;Completado&apos;)</expression>
    </formulas>
    <formulas>

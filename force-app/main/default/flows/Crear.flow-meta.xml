        <dataType>String</dataType>
        <expression>{!Recuperar_Scheduled.Name}&amp;&quot;-&quot;&amp;{!Recuperar_Program_Enrolment.Name}</expression>
    </formulas>
    <formulas>
        <name>numeroSocioALUMNO</name>
        <dataType>String</dataType>
